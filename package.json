{
  "name": "n8n-nodes-onprintshop",
  "version": "1.0.0",
  "description": "n8n node for OnPrintShop integration",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "onprintshop",
    "printing",
    "print-shop",
    "e-commerce"
  ],
  "license": "MIT",
  "homepage": "https://github.com/cderamos-2ct/n8n-nodes-onprintshop#readme",
  "author": {
    "name": "cderamos-2ct",
    "email": "your-email@example.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cderamos-2ct/n8n-nodes-onprintshop"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/OnPrintShopApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/OnPrintShop/OnPrintShop.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "gulp": "^4.0.2",
    "typescript": "^4.9.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}

